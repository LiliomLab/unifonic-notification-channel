{
  "name": "multicaret/unifonic-notification-channel",
  "description": "Unifonic Notification Channel for laravel.",
  "keywords": [
    "laravel",
    "notifications",
    "unifonic"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Kawsara",
      "email": "mkwsra@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "multicaret/laravel-unifonic": "^2.0"
  },
  "require-dev": {
    "illuminate/notifications": "~v8.35.1",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^9.5.0"
  },
  "autoload": {
    "psr-4": {
      "Multicaret\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Multicaret\\Tests\\Notifications\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Multicaret\\Notifications\\UnifonicChannelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
