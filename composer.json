{
  "name": "liliom/unifonic-notification-channel",
  "description": "Unifonic Notification Channel for laravel.",
  "keywords": [
    "laravel",
    "notifications",
    "unifonic"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammad Kawsara",
      "email": "mkwsra@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "liliom/laravel-unifonic": "^1.0"
  },
  "require-dev": {
    "illuminate/notifications": "~5.7",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Liliom\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Liliom\\Tests\\Notifications\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Liliom\\Notifications\\UnifonicChannelServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
